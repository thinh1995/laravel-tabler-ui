{
  "name": "lucifer293/laravel-tabler-ui",
  "description": "Tabler UI Blade for Laravel",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Lucifer\\LaravelTablerUi\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Lucifer\\LaravelTablerUi\\Tests\\": "tests"
    }
  },
  "authors": [
    {
      "name": "Nguyen Xuan Thinh",
      "email": "cuongthinhtuan2006@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "illuminate/support": "^v10.48.28"
  },
  "require-dev": {
    "orchestra/testbench": "^8.0",
    "phpunit/phpunit": "^10.5",
    "squizlabs/php_codesniffer": "^3.11",
    "friendsofphp/php-cs-fixer": "^3.71"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Lucifer\\LaravelTablerUi\\LaravelTablerUiProvider"
      ]
    }
  },
  "scripts": {
    "test": "phpunit",
    "phpcs": "phpcs"
  }
